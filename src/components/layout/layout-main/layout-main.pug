include ../../ui/ui.pug
include ../../head/head.pug
include ../../foot/foot.pug
include ../../header/header.pug
include ../../footer/footer.pug

doctype html
html(class="no-js", lang="ru")
  +head(data)
  body.page
    div.page__wrapper
      div.page__container
        div.page__header
          +header(data)
        div.page__content
          main.page__main
            block main

        div.page__footer
          +footer(data)

    +foot(data)
